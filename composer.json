{
    "name": "shobi/weatherapp",
    "description": "a small php cli to consume openwathermap api",
    "authors": [
        {
            "name": "Shobi P P",
            "email": "ppshobi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-2": {
            "Shobi\\Weather\\": "src"
        }
    },
    "autoload-dev": {
        "psr-2": {
            "Shobi\\Weather\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
   
}
